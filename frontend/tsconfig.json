{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "strict": true,
        "jsx": "react",
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "baseUrl": "."
    },
    "include": [
        "src"
    ]
}
